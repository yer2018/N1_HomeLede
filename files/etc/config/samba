
config samba
	option workgroup 'WORKGROUP'
	option name 'N1'
	option description 'Phicomm-N1'
	option homes '0'
	option autoshare '1'

config sambashare
	option browseable 'yes'
	option name 'TV'
	option path '/mnt/mmcblk2p4/cloudnas/TV'
	option read_only 'no'
	option guest_ok 'yes'

config sambashare
	option browseable 'yes'
	option name 'DL'
	option path '/mnt/mmcblk2p4/Downloads'
	option read_only 'no'
	option guest_ok 'yes'

